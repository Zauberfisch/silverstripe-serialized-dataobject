{
	"name": "zauberfisch/silverstripe-serialized-dataobject",
	"description": "SilverStripe database field that allows saving arbitrary data in a single db field using serialization",
	"type": "silverstripe-vendormodule",
	"keywords": [
		"silverstripe",
		"serialize",
		"dataobject",
		"dbfield",
		"multivalue",
		"multivaluefield",
		"json",
		"gridfield",
		"arraylist"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Zauberfisch",
			"email": "code@zauberfisch.at"
		}
	],
	"require": {
		"php": "^7 || ^8",
		"ext-json": "*",
		"silverstripe/framework": "^4.5",
		"silverstripe/vendor-plugin": "*"
	},
	"extra": {
		"expose": [
			"css",
			"javascript",
			"images"
		]
	},
	"autoload": {
		"psr-4": {
			"zauberfisch\\SerializedDataObject\\": "src/"
		}
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
